{
  "name": "frontline-site",
  "description": "Frontline Website",
  "version": "0.0.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "eslint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "stylelint": "stylelint --ignore-path .gitignore \"src/**/*.+(css|scss|sass)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "format-styles": "prettier --ignore-path .gitignore \"src/**/*.+(css|scss|sass)\" --write",
    "analyze": "ANALYZE=true next build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-icon-font": "npx icon-font-generator src/design-system/icons/source/*.svg -o src/design-system/icons/out --html=false --types=woff2 --json=false"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm run eslint --fix",
      "npm run format"
    ],
    "./src/**/*.{css,scss,sass}": [
      "npm run stylelint --fix",
      "npm run format-styles"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@mdx-js/loader": "^1.6.5",
    "@next/mdx": "^9.4.4",
    "@theme-ui/color": "^0.3.1",
    "@theme-ui/presets": "^0.3.0",
    "@theme-ui/prism": "^0.4.0-alpha.3",
    "change-case": "^4.1.1",
    "feather-icons": "^4.28.0",
    "next": "^9.4.2",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "remark-autolink-headings": "^6.0.0",
    "remark-slug": "^6.0.0",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@akqa-frontline/eslint-config-frontline": "^0.7.1",
    "@akqa-frontline/scss-config-webpack-plugin": "^0.7.1",
    "@akqa-frontline/stylelint-config-frontline": "^0.7.1",
    "@babel/core": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@next/bundle-analyzer": "^9.4.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@svgr/webpack": "^5.4.0",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/theme-ui": "^0.3.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "husky": "^4.2.3",
    "iconfont-webpack-plugin": "^4.2.2",
    "lint-staged": "^10.0.8",
    "node-sass-glob-importer": "^5.3.2",
    "node-sass-json-importer": "^4.3.0",
    "polished": "^3.6.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "prettier": "^1.19.1",
    "react-json-view": "^1.19.1",
    "sass": "^1.26.7",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.17.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
